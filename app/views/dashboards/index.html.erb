
<% @all_goals.each do |g| %>


   <div class="row">
    <div class="col s12 m8 offset-m2 l6 offset-l3">
      <div class="card hoverable">
        <div class="card-image dashboard-card">

          <div><%= link_to g.name, "/goals/#{g.id}"%></div>

          <div><%= g.created_at.strftime("%-d %b %Y") %></div>


        </div>

      </div>
    </div>
  </div>

<% end %>
